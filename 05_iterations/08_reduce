const shoppingcart=[
    { 
        itemname: "jscouse",
        price: 299
    },

    {
        itemname: "pycourse",
        price:499
    },

    {
        itemname: "javacourse",
        price:999
    }
]


const pricetopay=shoppingcart.reduce((acc,item)=> acc+item.price,0)
console.log(pricetopay)